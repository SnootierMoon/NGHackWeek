                                            function validateTenderForm(reqForm, successBanner, failureBanner, rfiFormContainer, formName) {
                                                reqForm.validate({
                                                    ignore: false,
                                                    onfocusout: function (element) {
                                                        if (['emNumberInput', 'fmNumberInput'].includes($(element).attr('id'))) {
                                                            $('#emNumberInput').valid()
                                                            $('#fmNumberInput').valid()
                                                        } else {
                                                            if ($(element).attr('data-type') !== 'datepicker') {
                                                                $(element).valid()
                                                            }
                                                        }
                                                    },
                                                    errorClass: "is-invalid text-danger",
                                                    validClass: 'is-valid',
                                                    invalidHandler: function (e, validator) {
                                                        // loop through the errors:
                                                        for (var i = 0; i < validator.errorList.length; i++) {
                            // "uncollapse" section containing invalid input/s:
                            $(validator.errorList[i].element).closest('.collapse').collapse('show');
                            }
                            },
                            submitHandler: function (form) {
                            failureBanner.addClass('d-none').removeClass('d-block');
                            successBanner.addClass('d-none').removeClass('d-block');
                            trackFormSubmission(formName)
                            $.LoadingOverlay("show");
                            $(form).ajaxSubmit({
                            error: function (xhr) {
                            $.LoadingOverlay("hide");
                            trackError('Form submission error', formName + ' | network error')
                            successBanner.addClass('d-none').removeClass('d-block');
                            failureBanner.addClass('d-block').removeClass('d-none');
                            failureBanner.html('<strong>Oh snap!</strong> There was an error processing your request, please try again or contact us at <a class="u-danger" href="mailto:info@satsearch.com?subject=RFI%20through%20satsearch.co">info@satsearch.com</a>!')
                                                                                                                                                                            },
                                                                                                                                                                            success: function (response) {
                                                                                                                                                                            $.LoadingOverlay("hide");
                                                                                                                                                                            if (response.code == 0) {
                                                                                                                                                                            $(form).clearForm();
                                                                                                                                                                            failureBanner.addClass('d-none').removeClass('d-block');
                                                                                                                                                                            $("#successForm").submit();
                                                                                                                                                                            } else if (response.code == 1) {
                                                                                                                                                                            trackError('Form submission error', formName + ' | captcha validation failed')
                                                                                                                                                                            successBanner.addClass('d-none').removeClass('d-block');
                                                                                                                                                                            failureBanner.addClass('d-block').removeClass('d-none');
                                                                                                                                                                            failureBanner.html('<strong>Oh snap!</strong> Captcha validation failed!')
                                                                                                                                                                            } else if (response.code == 3) {
                                                                                                                                                                            trackError('Form submission error', formName + ' | confirmation email error')
                                                                                                                                                                            failureBanner.addClass('d-none').removeClass('d-block');
                                                                                                                                                                            $(form).clearForm();
                                                                                                                                                                            $("#successForm").submit();
                                                                                                                                                                            } else if (response.code == 4) {
                                                                                                                                                                            successBanner.addClass('d-none').removeClass('d-block');
                                                                                                                                                                            failureBanner.addClass('d-block').removeClass('d-none');
                                                                                                                                                                            failureBanner.html('<strong>Oh snap!</strong> The data you submitted is not valid, please try again.');
                                                                                                                                                                            }
                                                                                                                                                                            }
                                                                                                                                                                            });
                                                                                                                                                                            },
                                                                                                                                                                            rules: {
                                                                                                                                                                            comments: {
                                                                                                                                                                            required: true,
                                                                                                                                                                            minlength: 10
                                                                                                                                                                            },
                                                                                                                                                                            firstName: {
                                                                                                                                                                            required: true
                                                                                                                                                                            },
                                                                                                                                                                            lastName: {
                                                                                                                                                                            required: true
                                                                                                                                                                            },
                                                                                                                                                                            email: {
                                                                                                                                                                            required: true,
                                                                                                                                                                            email: true
                                                                                                                                                                            },
                                                                                                                                                                            projectLifecycle: {
                                                                                                                                                                            required: '#quotationInput:checked'
                                                                                                                                                                            },
                                                                                                                                                                            organization: {
                                                                                                                                                                            required: true,
                                                                                                                                                                            maxlength: 200
                                                                                                                                                                            },
                                                                                                                                                                            newsletter: {
                                                                                                                                                                            required: true
                                                                                                                                                                            },
                                                                                                                                                                            anonymousRequester: {
                                                                                                                                                                            required: true
                                                                                                                                                                            },
                                                                                                                                                                            "interestedIn[]": {
                                                                                                                                                                            required: true,
                                                                                                                                                                            minlength: 1
                                                                                                                                                                            },
                                                                                                                                                                            deliveryDate: {
                                                                                                                                                                            required: false,
                                                                                                                                                                            dateISO: true
                                                                                                                                                                            },
                                                                                                                                                                            missionDate: {
                                                                                                                                                                            required: false,
                                                                                                                                                                            dateISO: true
                                                                                                                                                                            },
                                                                                                                                                                            emNumber: {
                                                                                                                                                                            number: true,
                                                                                                                                                                            required: false,
                                                                                                                                                                            min: 0
                                                                                                                                                                            },
                                                                                                                                                                            fmNumber: {
                                                                                                                                                                            number: true,
                                                                                                                                                                            required: false,
                                                                                                                                                                            min: 0

                                                                                                                                                                            },
                                                                                                                                                                            rfiCountry: {
                                                                                                                                                                            required: true
                                                                                                                                                                            },
                                                                                                                                                                            requesterRole: {
                                                                                                                                                                            required: true
                                                                                                                                                                            }
                                                                                                                                                                            }, //end rules
                                                                                                                                                                            messages: {
                                                                                                                                                                            comments: {
                                                                                                                                                                            required: 'Please fill out your request',
                                                                                                                                                                            minlength: 'Please provide more details'
                                                                                                                                                                            },
